{
  "compileOnSave": true,
  "compilerOptions": {
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "module": "es2020",
    "lib": ["es2020", "dom", "DOM.Iterable"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "public"
  },
}
